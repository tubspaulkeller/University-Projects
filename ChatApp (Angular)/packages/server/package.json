{
  "name": "server",
  "private": true,
  "version": "0.0.0",
  "types": "src/index.ts",
  "main": "src/index.ts",
  "scripts": {
    "start": "node ./dist/index.js",
    "start:watch": "nodemon ./dist/index.js",
    "build": "esbuild ./src/index.js --bundle --platform=node --outfile=dist/index.js",
    "build:watch": "yarn build --watch",
    "dev": "run-p start:watch build:watch",
    "typecheck": "tsc",
    "lint": "eslint .",
    "lint:fix": "yarn lint:fix",
    "test": "jest --passWithNoTests",
    "test:watch": "yarn test -- --watch",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^8.5.9",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.32.0",
    "joi": "^17.6.2",
    "joi-to-typescript": "^4.0.6",
    "jsonwebtoken": "^8.5.1",
    "uuid": "^9.0.0",
    "@hapi/joi": "^17.1.1",
    "body-parser": "^1.20.1",
    "dotenv": "^16.0.3",
    "express": "^4.17.1",
    "express-ws": "^5.0.2",
    "mongoose": "^6.6.5"
  },
  "devDependencies": {
    "@types/express": "4.17.14",
    "@types/express-ws": "^3.0.1",
    "@types/jest": "29.1.2",
    "@types/node": "18.8.3",
    "esbuild": "0.15.10",
    "eslint": "8.25.0",
    "eslint-config-custom": "*",
    "jest": "29.1.2",
    "jest-config": "*",
    "nodemon": "2.0.20",
    "npm-run-all": "4.1.5",
    "typescript": "4.8.4",
    "shared": "*"
  }
}
