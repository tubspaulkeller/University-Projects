{
  "name": "mdw-project",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean && rm -rf node_modules",
    "dev": "turbo run dev --no-cache --parallel --continue",
    "format": "prettier --write \"**/*.{ts,tsx,md,js,jsx,json}\"",
    "lint": "turbo run lint",
    "lint:changed": "turbo run lint:fix --filter=[HEAD]",
    "lint:fix": "turbo run lint:fix",
    "test": "turbo run test",
    "client": "yarn workspace client",
    "server": "yarn workspace server",
    "shared": "yarn workspace shared",
    "eslint-config": "yarn workspace eslint-config",
    "jest-config": "yarn workspace jest-config",
    "tsconfig": "yarn workspace tsconfig"
  },
  "devDependencies": {
    "prettier": "2.7.1",
    "turbo": "1.5.1"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=16.0.0"
  },
  "packageManager": "yarn@1.22.19"
}
